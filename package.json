{
  "name": "trgraph",
  "version": "1.0.2",
  "description": "transformation graph",
  "author": "pavelgoodpal",
  "license": "(Apache-2.0 AND MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcred-org/trgraph.git"
  },
  "keywords": [
    "transformation graph",
    "trgraph",
    "tr-graph"
  ],
  "type": "module",
  "scripts": {
    "build": "rm -r dist & npm run scripts && tsc -p tsconfig.build.json",
    "test": "npx tsm ./node_modules/uvu/bin.js ./test",
    "dev": "npm run build && node ./dist/main.js",
    "scripts": "npx tsm scripts/create-link-types.ts",
    "publish": "npm run build && npm publish"
  },
  "files": [
    "./dist"
  ],
  "dependencies": {
    "ieee754": "^1.2.1",
    "sort-keys": "^5.0.0",
    "uint8arrays": "^4.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "tsm": "^2.3.0",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.t"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
